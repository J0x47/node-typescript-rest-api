{
  "name": "typescript-rest-api",
  "version": "1.0.0",
  "description": "node.js express typescript rest api",
  "main": "server.js",
  "scripts": {
    "start_local": "node --inspect=5858 -r ts-node/register ./src/server.ts",
    "start": "npm run build && node ./dist/server.js",
    "start:watch": "nodemon",
    "build": "tsc",
    "lint": "eslint --ext .ts .",
    "test": "jest"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "eslint",
    "prettier"
  ],
  "author": "Jason",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.11",
    "@types/jest": "^26.0.21",
    "@types/node": "^14.14.35",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.19.0",
    "@typescript-eslint/parser": "^4.19.0",
    "eslint": "^7.22.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-prettier": "^3.3.1",
    "jest": "^26.6.3",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "express": "^4.17.1"
  }
}
